{
  "name": "jsreport-data",
  "version": "1.0.2",
  "description": "jsreport extension adding support for using sample data for previewing templates",
  "scripts": {
    "test": "mocha test --timeout 5000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-data",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-data.git"
  },
  "keywords": [
    "jsreport",
    "data"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "license": "LGPL",
  "dependencies": {    
    "in-publish": "2.0.0",    
    "shortid": "2.2.6"    
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "eslint": "3.14.1",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-plugin-babel": "4.0.1",
    "jsreport-core": "1.0.10",
    "jsreport-studio-dev": "1.0.2",
    "jsreport-templates": "1.0.2",
    "mocha": "3.2.0",
    "standard": "8.6.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
